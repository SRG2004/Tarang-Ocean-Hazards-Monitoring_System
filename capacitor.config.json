{
  "appId": "com.incois.taranga",
  "appName": "Taranga",
  "webDir": "build",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "Geolocation": {
      "permissions": [
        "location"
      ]
    },
    "Camera": {
      "permissions": [
        "camera",
        "photos"
      ]
    }
  }
}